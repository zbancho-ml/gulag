COMMAND_PREFIX=z!

SEASONAL_BGS=https://pobierak.jeja.pl/images_thumb/5/2/7/703728_600x315.jpg

MENU_ICON_URL=https://i.kym-cdn.com/entries/icons/mobile/000/029/591/Screen_Shot_2019-05-06_at_1.52.03_PM.jpg
MENU_ONCLICK_URL=https://akatsuki.pw


DATADOG_API_KEY=
DATADOG_APP_KEY=

